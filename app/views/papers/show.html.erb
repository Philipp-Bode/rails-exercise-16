<p>
  <strong>Title:</strong>
  <%= @paper.title %>
</p>
 
<p>
  <strong>Venue:</strong>
  <%= @paper.venue %>
</p>

<p>
  <strong>Year:</strong>
  <%= @paper.year %>
</p>
<h2> Authors: </h2>
<% @paper.authors.each do |author| %>
	<p>
	  <strong>Author:</strong>
	  <%= author.name %>
	</p>	
<% end %>